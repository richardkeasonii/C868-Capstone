<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.helloworldjfxtemplate.controller.LoginController">
      <children>
              <AnchorPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="385.0" prefWidth="640.0" VBox.vgrow="ALWAYS">
                        <children>
                            <Label fx:id="loginLabel" layoutX="60.0" layoutY="30.0" prefHeight="39.0" prefWidth="172.0" text="Login Form">
               <font>
                  <Font size="20.0" />
               </font></Label>
                            <Label fx:id="zoneLabel" layoutX="412.0" layoutY="30.0" prefHeight="39.0" prefWidth="172.0" text="Zone" />
                            <TextField fx:id="usernameField" layoutX="146.0" layoutY="188.0" onAction="#onClickUsername" promptText="Username" />
                            <TextField fx:id="passwordField" layoutX="349.0" layoutY="188.0" onAction="#onClickPassword" promptText="Password" />
                            <Button fx:id="submitButton" layoutX="295.0" layoutY="280.0" onAction="#onClickSubmit" text="Submit" />
                        </children>
              </AnchorPane>
      </children>
</VBox>
